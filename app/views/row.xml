<Alloy>
	<TableViewRow id="row" touchEnabled="false">
		<ImageView id="fav" onClick="toggleFav" user_id="{user_id}" done="{done}" />
		<Label id="task" text="{item}" ph="{date_completed}" user_id="{user_id}" email="{email}" ppic="{ppic}" done="{done}"/>
		<Label id="subtask" text="{date_completed}" touchEnabled="false" />
		<Button class="databtn"  onLongpress="deleteTask" user_id="{user_id}">Delete</Button>
	</TableViewRow>
</Alloy>